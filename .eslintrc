{
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "globals": {
    "gairal": true
  },
  "plugins": ["jest"],
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": 1,
    "curly": 2,
    "indent": [2, 2, { "SwitchCase": 1 }],
    "eqeqeq": 2,
    "new-cap": 2,
    "camelcase": 2,
    "quotes": [2, "single"],
    "no-trailing-spaces": 2
  }
}
