{
  "name": "webpack-stack",
  "version": "1.0.0",
  "description": "Front-end Stack with Pug, Webpack, Babel, ES2016, SASS, Jest",
  "author": "Gairal",
  "readmeFilename": "README.md",
  "main": "src/app/index.js",
  "keywords": [
    "Front-end",
    "Pug",
    "Sass",
    "css",
    "JavaScript",
    "Webpack",
    "babel",
    "es6",
    "es2015",
    "jest"
  ],
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.config.js",
    "debug": "./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.config.debug.js",
    "build": "./node_modules/.bin/webpack --config ./webpack/webpack.config.js",
    "release": "./node_modules/.bin/webpack --config ./webpack/webpack.config.prod.js",
    "lint": "./node_modules/.bin/eslint ./src/ && ./node_modules/.bin/eslint ./test/ && ./node_modules/.bin/stylelint './src/**/*.scss'",
    "test": "./node_modules/.bin/jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all lint test",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@commitlint/cli": "^7.1.0",
    "@commitlint/config-conventional": "^7.1.0",
    "autoprefixer": "^9.1.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^0.1.0",
    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.0",
    "eslint-import-resolver-alias": "^1.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.11.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.13",
    "imagemin-webpack-plugin": "^2.2.0",
    "jest": "^23.6.0",
    "jest-localstorage-mock": "^2.2.0",
    "jest-mock-console": "^0.4.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.0",
    "prettier": "^1.14.0",
    "postcss-loader": "^3.0.0",
    "postcss-reporter": "^6.0.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-concentric-order": "^3.4.0",
    "stylelint-config-sass-guidelines": "^5.2.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.0",
    "stylelint-selector-bem-pattern": "^2.0.0",
    "stylelint-webpack-plugin": "~0.10.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.18.0",
    "webpack-bundle-analyzer": "^2.13.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.0",
    "write-file-webpack-plugin": "^4.4.0"
  },
  "dependencies": {
    "autotrack": "~2.4.0",
    "chooles": "^0.0.6",
    "es6-promise": "^4.2.0",
    "loggout": "^0.0.2",
    "tailwindcss": "^0.6.0",
    "whatwg-fetch": "^3.0.0"
  }
}
