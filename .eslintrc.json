{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "mocha": true
  },
  "plugins": [
    "mocha"
  ],
  "extends": "airbnb",
  "rules": {
    "curly": 2,
    "indent": [2, 2, { "SwitchCase": 1 }],
    "eqeqeq": 2,
    "new-cap": 2,
    "camelcase": 2,
    "quotes": [2, "single"],
    "no-trailing-spaces": 2,
    "no-console": 1
  }
}
